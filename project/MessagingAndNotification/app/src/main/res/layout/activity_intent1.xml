<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="kaleidot725.messagingandnotification.IntentActivity1">


    <Button
        android:id="@+id/CallActivity1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/activity_call_1"/>

    <Button
        android:id="@+id/CallActivity2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/CallActivity1"
        android:text="@string/activity_call_2"/>

    <Button
        android:id="@+id/CallActivity3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/CallActivity2"
        android:text="@string/activity_call_3"/>
</RelativeLayout>
